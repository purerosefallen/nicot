{
  "name": "nicot",
  "description": "Nest.js interacting with class-validator + OpenAPI + TypeORM for Nest.js Restful API development.",
  "version": "1.1.13",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "lint": "eslint --fix .",
    "build": "rimraf dist && tsc",
    "test": "jest --passWithNoTests",
    "start": "node dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/purerosefallen/nicot.git"
  },
  "author": "Nanahira <nanahira@momobako.com>",
  "license": "MIT",
  "keywords": [
    "typescript",
    "typeorm",
    "nest",
    "nestjs",
    "rest",
    "restful",
    "api",
    "crud",
    "crud-generator",
    "backend",
    "frameworks"
  ],
  "bugs": {
    "url": "https://github.com/purerosefallen/nicot/issues"
  },
  "homepage": "https://github.com/purerosefallen/nicot",
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "tests",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "devDependencies": {
    "@nestjs/core": "^11.0.14",
    "@nestjs/platform-express": "^11.0.14",
    "@nestjs/testing": "^11.0.14",
    "@nestjs/typeorm": "^11.0.0",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.14.182",
    "@types/node": "^22.15.2",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^3.4.1",
    "jest": "^29.7.0",
    "pg": "^8.7.3",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "supertest": "^6.2.4",
    "ts-jest": "^29.3.2",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "@nestjs/common": "^11.0.0 || ^9.4.0 || ^10.0.0",
    "@nestjs/config": "^4.0.0 || ^3.0.0",
    "@nestjs/swagger": "^11.0.0 || ^7.1.8 || ^6.3.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "typeorm": "^0.3.16"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "nesties": "^1.1.2",
    "superjson": "1.13.3",
    "typed-reflector": "^1.0.11"
  }
}
